/* pages/Shop_process/winning/winning.wxss */
.top_block{
  padding: 35rpx 40rpx;
  background: #fff;
  font-weight:500;
}
.activity_header{
  width:100%;
  height:110rpx;
  background:rgba(198,205,224,1);
  padding: 23rpx 40rpx;
}
.activity_header.active{
  background:linear-gradient(-52deg,rgba(227,21,54,1),rgba(245,81,0,1));
}
.duihuan_type text{
  margin-right: 20rpx;
}
.duihuan_type text:last-child{margin-right: 0}
.win_list{
  background: #fff;
  padding: 0 !important;
}
.list_content{
  text-align: center;
  padding: 10rpx 20rpx;
  font-size: 24rpx;
  color: #a5afc5;
  line-height: 60rpx;
}
.list_header{
  height: 70rpx;
  line-height: 70rpx;
  border-bottom: 1rpx solid #dee0e5;
  font-size:26rpx;
  font-weight:500;
  color: #272f45;
}
.win_list_item>view,.list_header>view{
  overflow: hidden; white-space: nowrap; text-overflow: ellipsis;
}
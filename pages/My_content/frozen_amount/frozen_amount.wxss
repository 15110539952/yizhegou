@import "/pages/My_content/my_money/my_money.wxss";
.top_block{
  background: #fff;
  padding: 30rpx 20rpx 50rpx;
}
.money_block{
	width: 100%;
	height: 180rpx;
  position: relative;
  padding-top: 50rpx;
}
.top_bg{
	border-radius: 10rpx;
  position: absolute;
  top: 0;left: 0;
  width: 100%;
  height: 100%;
}
.money_content{
  width: 100%;
  position: relative;
  z-index: 9;
  color: #fff;
  justify-content: space-around;
  flex-direction: column;
  padding-left: 50rpx;
}
